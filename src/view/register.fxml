<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RegisterController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Text layoutX="266.0" layoutY="100.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Register">
               <font>
                  <Font size="36.0" />
               </font>
            </Text>
            <TextField fx:id="nameRegister" layoutX="257.0" layoutY="139.0" promptText="Name" />
            <TextField fx:id="usernameRegister" layoutX="256.0" layoutY="200.0" promptText="Username" />
            <TextField fx:id="passwordRegister" layoutX="257.0" layoutY="240.0" promptText="Password" />
            <TextField fx:id="emailRegister" layoutX="257.0" layoutY="278.0" promptText="Email Address" />
            <Button fx:id="registerButton" layoutX="248.0" layoutY="361.0" mnemonicParsing="false" onAction="#registerCheck" prefHeight="11.0" prefWidth="166.0" text="Register" />
            <Button fx:id="loginReturn" layoutX="523.0" layoutY="49.0" mnemonicParsing="false" onAction="#returnToLogin" text="Return To Login" />
         </children>
    </AnchorPane>
  </children>
</AnchorPane>
