<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.photoBrowserController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" />
      <TabPane fx:id="tabPane" prefHeight="400.0" prefWidth="640.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="photosPane" text="Photos">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="loginReturnPhotos" layoutX="564.0" layoutY="331.0" mnemonicParsing="false" onAction="#returnToLogin" prefHeight="26.0" prefWidth="62.0" text="Log Off" />
                        <ImageView fitHeight="194.0" fitWidth="259.0" layoutX="367.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
                        <TableView fx:id="photoList" layoutX="12.0" layoutY="48.0" prefHeight="265.0" prefWidth="616.0" AnchorPane.bottomAnchor="58.0" AnchorPane.leftAnchor="12.0" AnchorPane.rightAnchor="12.0" AnchorPane.topAnchor="48.0">
                          <columns>
                            <TableColumn fx:id="imageTitle" prefWidth="75.0" text="Title" />
                            <TableColumn fx:id="imageDescription" prefWidth="75.0" text="Description" />
                            <TableColumn fx:id="photoImage" prefWidth="464.0" text="Image" />
                          </columns>
                        </TableView>
                        <TextField fx:id="searchTagBox" layoutX="14.0" layoutY="14.0" promptText="Search for Tags" />
                        <Button fx:id="searchButton" layoutX="172.0" layoutY="14.0" mnemonicParsing="false" onAction="#search" text="Search" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab fx:id="ActivePhotoPane" text="Photo">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="loginReturnSearchTags" layoutX="564.0" layoutY="331.0" mnemonicParsing="false" onAction="#returnToLogin" prefHeight="26.0" prefWidth="62.0" text="Log Off" />
                        <Button fx:id="activePhotoBackButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToPhotoList" text="Back" />
                        <TextArea fx:id="activePhotoDescription" layoutX="35.0" layoutY="131.0" prefHeight="200.0" prefWidth="200.0" promptText="No description" />
                        <TextArea fx:id="activePhotoTitle" layoutX="35.0" layoutY="77.0" prefHeight="19.0" prefWidth="200.0" promptText="No Title" />
                        <ImageView fx:id="activePhoto" fitHeight="295.0" fitWidth="386.0" layoutX="244.0" layoutY="27.0" pickOnBounds="true" preserveRatio="true" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab fx:id="InsertPhotoPane" text="Insert Photo">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="selectPhotoButton" layoutX="74.0" layoutY="48.0" mnemonicParsing="false" onAction="#selectPhoto" text="Select Photo" />
                        <TextField fx:id="uploadImageTitle" layoutX="41.0" layoutY="109.0" promptText="Title" />
                        <TextArea fx:id="uploadImageDescription" layoutX="15.0" layoutY="156.0" prefHeight="88.0" prefWidth="200.0" promptText="Description" />
                        <TextField fx:id="uploadImageTag" layoutX="14.0" layoutY="266.0" prefHeight="25.0" prefWidth="107.0" promptText="Add Tag" />
                        <Button fx:id="addButton" layoutX="123.0" layoutY="266.0" mnemonicParsing="false" onAction="#addTag" text="Add" />
                        <ImageView fx:id="uploadImageView" fitHeight="203.0" fitWidth="271.0" layoutX="355.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
                        <Button fx:id="uploadPhotoButton" layoutX="71.0" layoutY="319.0" mnemonicParsing="false" onAction="#uploadPhoto" text="Upload Photo" />
                        <Button fx:id="insertPhotoReturnToLogin" layoutX="564.0" layoutY="331.0" mnemonicParsing="false" onAction="#returnToLogin" prefHeight="26.0" prefWidth="62.0" text="Log Off" />
                        <TableView fx:id="TagListTable" layoutX="220.0" layoutY="55.0" prefHeight="290.0" prefWidth="113.0">
                          <columns>
                            <TableColumn fx:id="tagListInsert" prefWidth="112.0" text="Tag List" />
                          </columns>
                        </TableView>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
  </children>
</AnchorPane>
